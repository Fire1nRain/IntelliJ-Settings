          <Description>Previous Occurrence</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'SignUtils'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'SignUtils'</Description>
          <Count>6</Count>
        </Item>
