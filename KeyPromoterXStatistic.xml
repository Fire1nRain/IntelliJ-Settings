      <Statistic Action="Debug 'Tomcat 8.0'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'Tomcat 8.0'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
